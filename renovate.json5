{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  platform: "github",
  repositories: [
    "AkiKanellis/github-action-continuous-release-test"
  ],
  packageRules: [
    {
      matchDatasources: ["docker"],
      matchPackageNames: ["github/super-linter"],
      versioning: "\
regex:\
^((?<compatibility>.+)-)?\
v?\
(?<major>\\d+)\
(\\.(?<minor>\\d+))?\
(\\.(?<patch>\\d+))?$"
    }
  ]
}
