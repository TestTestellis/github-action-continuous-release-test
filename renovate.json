{
  "extends": [
    "config:base",

    ":automergeDigest",
    ":automergeMinor",
    ":maintainLockFilesWeekly",
    ":semanticCommitTypeAll(build)",

    "docker:pinDigests",

    "group:allNonMajor",

    "helpers:pinGitHubActionDigests",

    "schedule:weekends"
  ],
  "platform": "github",
  "repositories": ["AkiKanellis/github-action-continuous-release-test"],
  "branchPrefix": "update-dependencies-",
  "onboarding": false,
  "requireConfig": false,
  "platformAutomerge": true,
  "rollbackPrs": true,
  "regexManagers": [
    {
      "fileMatch": [
        "^Makefile$"
      ],
      "matchStrings": [
        "SUPER_LINTER_VERSION\\s*=\\s*(?<currentValue>.*)"
      ],
      "datasourceTemplate": "docker",
      "depNameTemplate": "github/super-linter",
      "versioningTemplate": "docker"
    }
  ]
}
